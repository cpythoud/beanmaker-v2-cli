<table xmlns="https://schema.beanmaker.org/beanmaker-table"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="https://schema.beanmaker.org/beanmaker-table ../../resources/beanmaker-table.xsd"
>
    <name>table-name</name>
    <package>com.java.package</package>
    <bean>BeanClassName</bean>
    <!-- showing only fields with non default configuration -->
    <fields>
        <field>
            <sql-name>id_customer</sql-name>
            <java-type>long</java-type>
            <java-name>idCustomer</java-name>
            <required>true</required>
            <unique>false</unique>
            <bean-class>Customer</bean-class>
        </field>
        <field>
            <sql-name>number</sql-name>
            <java-type>String</java-type>
            <java-name>number</java-name>
            <required>true</required>
            <unique>false</unique>
        </field>
        <field>
            <sql-name>printed_date</sql-name>
            <java-type>Date</java-type>
            <java-name>printedDate</java-name>
            <required>true</required>
            <unique>false</unique>
        </field>
    </fields>
    <relationships>
        <relationship>
            <database-table>items</database-table>
            <java-type>Item</java-type>
            <java-name>items</java-name>
            <id-field>id_invoice</id-field>
            <list-only>true</list-only>
        </relationship>
    </relationships>
    <extra-fields>
        <field>
            <java-type>MyType</java-type>
            <java-name>myTypeObject</java-name>
            <initialisation>new MyType(OtherType.CONSTANT)</initialisation>
            <final/>
            <import>com.company.software.MyType</import>
            <import>com.company.software.OtherType</import>
        </field>
        <field>
            <java-type>OtherType</java-type>
            <java-name>otherTypeObject</java-name>
            <initialisation>new OtherType()</initialisation>
            <final/>
        </field>
    </extra-fields>
</table>
